
{
	"version": "0.2.0",
	"configurations": [
		
		{
		"name": "Debug webserv",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/webserv",
		"args": [],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		{
		"description": "Enable pretty-printing for gdb",
		"text": "-enable-pretty-printing",
		"ignoreFailures": true
		}
		],
		"preLaunchTask": "build",
		"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}
